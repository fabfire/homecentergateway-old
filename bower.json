{
  "name": "nodejsprojecttemplate",
  "version": "0.1.0",
  "description": "Node.JS + Express template",
  "authors": "",
  "license": "MIT",
  "homepage": "https://bitbucket.org/fabfire/nodejsprojecttemplate/overview",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components"
  ],
  "dependencies": {
    "jquery": "^2.2.0",
    "bootstrap": "~3.3.6",
    "intl": "^1.1.0",
    "font-awesome": "~4.6.0",
    "kendo-ui": "kendo-ui-core#^2016.1.217",
    "moment": "~2.11.2",
    "PACE": "pace#^1.0.2",
    "toastr": "~2.1.2",
    "Ionicons": "ionicons#^2.0.1",
    "socket.io": "^1.4.5",
    "weather-icons": "^2.0.10"
  },
  "overrides": {
    "bootstrap": {
      "main": [
        "dist/js/bootstrap.js",
        "dist/css/bootstrap.css",
        "less/bootstrap.less"
      ]
    },
    "font-awesome": {
      "main": [
        "css/font-awesome.css",
        "css/font-awesome.less"
      ]
    },
    "PACE": {
      "main": [
        "pace.js",
        "themes/black/pace-theme-minimal.css"
      ]
    },
    "weather-icons": {
      "main": [
        "css/weather-icons.css",
        "font/*"
      ]
    }
  }
}
